# JDBC

public class Main {
	public static void main(String[] args) {
		DBManager manager = new DBManager();
		
		try {
			int result = manager.insertData("person7","lee@naver.com","kor",5);
			
			if(result>0) {
				System.out.println("success");
			} else {
				System.out.println("fail");
			}
			
		}catch(Exception e) {
			e.printStackTrace();
		}
	}
}
